{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "repositories": {
        "spatie-custom": {
            "type": "path",
            "url": "packages/ndc/spatie-custom",
            "options": {
                "symlink": true
            }
        },
        "font-beauty": {
            "type": "path",
            "url": "packages/fortune/font-beauty",
            "options": {
                "symlink": true
            }
        }
    },
    "require": {
        "php": "^8.2.13",
        "brian2694/laravel-toastr": "^5.57",
        "darkaonline/l5-swagger": "^8.5",
        "directorytree/ldaprecord-laravel": "^2.7.3",
        "doctrine/dbal": "^3.4",
        "firebase/php-jwt": "^6.10",
        "fortune/font-beauty": "@dev",
        "fruitcake/laravel-cors": "dev-develop",
        "guzzlehttp/guzzle": "^7.2",
        "intervention/image": "^2.7",
        "jenssegers/agent": "^2.6",
        "laravel/framework": "^10.0",
        "laravel/horizon": "^5.15",
        "laravel/sanctum": "^3.2",
        "laravel/scout": "^10.5",
        "laravel/socialite": "^5.12.1",
        "laravel/telescope": "^4.17.6",
        "laravel/tinker": "^2.9.0",
        "laravel/ui": "^4.4.0",
        "league/flysystem-aws-s3-v3": "3.0.22",
        "lkaemmerling/laravel-horizon-prometheus-exporter": "^1.7",
        "mobiledetect/mobiledetectlib": "^2.8",
        "ndc/spatie-custom": "@dev",
        "nuwave/lighthouse": "^5.61",
        "phpoffice/phpword": "^1.2",
        "predis/predis": "^2.1",
        "promphp/prometheus_client_php": "^2.7",
        "pusher/pusher-php-server": "^7.0",
        "smalot/pdfparser": "^2.10",
        "spatie/laravel-pdf": "^1.2",
        "spatie/laravel-permission": "^5.5",
        "stevebauman/location": "^6.6"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "files": [
            "app/Helpers/Helper.php",
            "app/Utils/utils.php"
        ],
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
