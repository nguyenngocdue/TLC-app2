docker exec -it laravel102_php php artisan event:clear

docker exec -it laravel102_php php artisan route:clear
docker exec -it laravel102_php php artisan route:cache

supervisorctl restart all

echo "Don't forget to run php artisan migrate to have the latest version of DB"